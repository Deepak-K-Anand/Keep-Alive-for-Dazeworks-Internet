{
  "name": "Keep Alive for Router Internet Session",
  "description": "Ensures that you're live on the Internet Session with Router!",
  "version": "0.1",
  "manifest_version": 2,
  "offline_enabled": false,
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },
  "permissions": [
    "webview",
    "storage"
  ],
  "icons": { 
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png" 
  }
}